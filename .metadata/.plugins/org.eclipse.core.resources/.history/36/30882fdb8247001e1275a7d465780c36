package br.com.senaisp.bauru.richard.licao03;

public class TesteEscopo2 {
	
	public static int valor = 10;

	public static void main(String[] args) {
		int valor = 5;
		System.out.println(valor);
		valor++;
		testeValor();
		System.out.println(valor);
	}

	private static void testeValor() {
		System.out.println(valor);
		valor++;
	}

}
