package br.com.senaisp.bauru.richard.licao02;

public class TesteEscopoVariavel {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int idade = 10;
		if (idade<=10) {
			String nome = "Roberto";
			System.out.println(nome);
		}
		System.out.println(idade);
		System.out.println(nome);
	}

}
